{
  "name": "@springworks/test-harness",
  "description": "Setup should and sinon with plugins",
  "main": "index.js",
  "author": "Springworks",
  "scripts": {
    "test": "mocha",
    "test-cov": "istanbul cover _mocha",
    "lint": "eslint .",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release",
    "commit": "commit-wizard"
  },
  "dependencies": {
    "@types/sinon": "^4.3.3",
    "checkdigit": "^1.1.1",
    "mocha-teamcity-reporter": "^2.2.0",
    "should": "^13.2.3",
    "should-http": "^0.1.0",
    "should-sinon": "^0.0.6",
    "sinon": "^7.2.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "coveralls": "^3.0.2",
    "eslint": "^5.0.0",
    "eslint-config-springworks": "^7.0.7",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^5.1.0",
    "eslint-plugin-should-promised": "^2.0.0",
    "eslint-plugin-springworks": "^2.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "pre-git": "^3.15.3",
    "semantic-release": "^15.9.5"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Springworks/node-test-harness.git"
  },
  "bugs": {
    "url": "https://github.com/Springworks/node-test-harness/issues"
  },
  "homepage": "https://github.com/Springworks/node-test-harness#readme",
  "engines": {
    "node": ">=6"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "parserOpts": {
        "noteKeywords": [
          "BREAKING CHANGE",
          "BREAKING CHANGES",
          "BREAKING"
        ]
      }
    }
  }
}
