{
  "name": "@springworks/test-harness",
  "description": "Setup should and sinon with plugins",
  "main": "index.js",
  "author": "Springworks",
  "scripts": {
    "test": "mocha",
    "test-cov": "istanbul cover _mocha",
    "lint": "eslint .",
    "postpublish": "greenkeeper-postpublish",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "commit-wizard"
  },
  "dependencies": {
    "should": "10.0.0",
    "should-http": "0.0.4",
    "should-sinon": "0.0.5",
    "sinon": "1.17.4",
    "mocha-teamcity-reporter": "1.0.2"
  },
  "devDependencies": {
    "babel-eslint": "6.1.2",
    "coveralls": "2.11.11",
    "eslint": "3.1.1",
    "eslint-config-springworks": "7.0.4",
    "eslint-plugin-import": "1.11.1",
    "eslint-plugin-mocha": "4.0.0",
    "eslint-plugin-should-promised": "1.0.8",
    "eslint-plugin-springworks": "2.0.1",
    "greenkeeper-postpublish": "1.0.0",
    "istanbul": "0.4.4",
    "mocha": "2.5.3",
    "pre-git": "3.10.0",
    "semantic-release": "4.3.5"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Springworks/node-test-harness.git"
  },
  "bugs": {
    "url": "https://github.com/Springworks/node-test-harness/issues"
  },
  "homepage": "https://github.com/Springworks/node-test-harness#readme"
}
