{
  "name": "@springworks/test-harness",
  "description": "Setup should and sinon with plugins",
  "main": "index.js",
  "author": "Springworks",
  "scripts": {
    "test": "mocha",
    "test-cov": "istanbul cover _mocha",
    "lint": "eslint .",
    "postpublish": "greenkeeper-postpublish",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "commit-wizard"
  },
  "dependencies": {
    "checkdigit": "1.1.1",
    "mocha-teamcity-reporter": "1.1.1",
    "should": "11.1.1",
    "should-http": "0.0.4",
    "should-sinon": "0.0.5",
    "sinon": "1.17.6",
    "sinon-as-promised": "4.0.2"
  },
  "devDependencies": {
    "babel-eslint": "7.1.0",
    "coveralls": "2.11.15",
    "eslint": "3.10.1",
    "eslint-config-springworks": "7.0.6",
    "eslint-plugin-import": "2.1.0",
    "eslint-plugin-mocha": "4.7.0",
    "eslint-plugin-should-promised": "2.0.0",
    "eslint-plugin-springworks": "2.0.1",
    "greenkeeper-postpublish": "1.0.1",
    "istanbul": "0.4.5",
    "mocha": "3.1.2",
    "pre-git": "3.10.0",
    "semantic-release": "4.3.5"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Springworks/node-test-harness.git"
  },
  "bugs": {
    "url": "https://github.com/Springworks/node-test-harness/issues"
  },
  "homepage": "https://github.com/Springworks/node-test-harness#readme"
}
